{
  "name": "slp-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "NODE_ENV=production && blitz build && blitz prisma migrate deploy",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.4",
    "@prisma/client": "3.2.1",
    "@tailwindcss/forms": "0.3.4",
    "@tippyjs/react": "4.2.6",
    "autoprefixer": "10",
    "blitz": "0.41.0",
    "chart.js": "3.6.0",
    "dayjs": "1.10.7",
    "faker": "5.5.3",
    "final-form": "4.20.4",
    "postcss": "8",
    "prisma-dbml-generator": "0.8.1",
    "react": "alpha",
    "react-chartjs-2": "3.3.0",
    "react-dom": "alpha",
    "react-final-form": "6.5.7",
    "react-hot-toast": "2.1.1",
    "react-spinners": "0.11.0",
    "react-spring": "9.3.0",
    "react-timer-hook": "3.0.5",
    "tailwindcss": "2.2.19",
    "zod": "3.9.8"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.3.0",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.29",
    "eslint": "7.32.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "0.15.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.5",
    "prisma": "3.2.1",
    "typescript": "~4.3"
  },
  "private": true
}
